{
  "name": "mobile",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite --port 3000 --host",
    "server": "node server.js",
    "build": "tsc && vite build",
    "build:analyze": "cross-env ANALYZE=true vite build",
    "preview": "vite preview --port 3000 --host",
    "preview:build": "vite preview --port 3000 --host",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "vercel-build": "yarn install --immutable --immutable-cache --check-cache && yarn build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-scroll": "^1.8.10",
    "@types/styled-components": "^5.1.34",
    "bootstrap": "^5.3.7",
    "chart.js": "^4.5.0",
    "express": "^5.1.0",
    "framer-motion": "^12.18.1",
    "http-proxy-middleware": "^3.0.5",
    "i18next": "^25.3.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "nodemailer": "^7.0.5",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.10",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.3",
    "react-scroll": "^1.9.3",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.4",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vercel/node": "^5.3.16",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
